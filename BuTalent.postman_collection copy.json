	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"let today = new Date();\r",
					"let randomOffset = Math.floor(Math.random() * 120) - 60; // Range -60 to +59\r",
					"today.setDate(today.getDate() + randomOffset);\r",
					"let formattedDate = today.toISOString().split('T')[0];\r",
					"\r",
					"pm.environment.set(\"date\", formattedDate);\r",
					"pm.environment.set(\"dueDate\", formattedDate);\r",
					"\r",
					"// 100000 ilə 300000 arasında təsadüfi rəqəm yaradılır\r",
					"function getRandomPrice(min, max) {\r",
					"    return Math.floor(Math.random() * (max - min + 1)) + min;\r",
					"}\r",
					"function generateVIN() {\r",
					"    let chars = \"ABCDEFGHJKLMNPRSTUVWXYZ0123456789\"; // VIN üçün uyğun simvollar\r",
					"    let vin = \"\";\r",
					"    for (let i = 0; i < 17; i++) {\r",
					"        vin += chars.charAt(Math.floor(Math.random() * chars.length));\r",
					"    }\r",
					"    return vin;\r",
					"}\r",
					"\r",
					"// Dəyəri Postman environment dəyişəni kimi saxlayırıq\r",
					"pm.environment.set(\"price\", getRandomPrice(100000, 300000));\r",
					"pm.environment.set(\"concessionPrice\", getRandomPrice(2000, 30000));\r",
					"pm.environment.set(\"orderStatus\", getRandomPrice(1, 6));\r",
					"pm.environment.set(\"orderStatus2\", getRandomPrice(1, 6));\r",
					"// VIN dəyərini Postman environment dəyişəninə yazırıq\r",
					"pm.environment.set(\"factoryInvoiceVin\", generateVIN());\r",
					"\r",
					""
				]
			}
		},
