				"type": "text/javascript",
				"exec": [
					"let today = new Date();",
					"let randomOffset = Math.floor(Math.random() * 120) - 60;",
					"today.setDate(today.getDate() + randomOffset);",
					"let formattedDate = today.toISOString().split('T')[0];",
					"pm.environment.set(\"date\", formattedDate);",
					"pm.environment.set(\"dueDate\", formattedDate);",
					"function getRandomPrice(min, max) {",
					"    return Math.floor(Math.random() * (max - min + 1)) + min;",
					"}",
					"function generateVIN() {",
					"    let chars = \"ABCDEFGHJKLMNPRSTUVWXYZ0123456789\";",
					"    let vin = \"\";",
					"    for (let i = 0; i < 17; i++) {",
					"        vin += chars.charAt(Math.floor(Math.random() * chars.length));",
					"    }",
					"    return vin;",
					"}",
					"pm.environment.set(\"price\", getRandomPrice(100000, 300000));",
					"pm.environment.set(\"concessionPrice\", getRandomPrice(2000, 30000));",
					"pm.environment.set(\"orderStatus\", getRandomPrice(1, 6));",
					"pm.environment.set(\"orderStatus2\", getRandomPrice(1, 6));",
					"pm.environment.set(\"factoryInvoiceVin\", generateVIN());"
				]
			}
		}
